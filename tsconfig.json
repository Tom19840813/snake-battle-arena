{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["./src/*"],
      "src/game/*": ["./src/game/*"],
      "src/ai/*": ["./src/ai/*"],
      "src/utils/*": ["./src/utils/*"],
      "src/powers/*": ["./src/powers/*"],
      "./types": ["./src/game/types.ts"]
    },
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "allowJs": true,
    "noUnusedLocals": false,
    "strictNullChecks": false
  }
}
